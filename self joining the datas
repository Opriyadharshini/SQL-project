CREATE TABLE employee_manager (
    eid INT PRIMARY KEY,
    name VARCHAR(50),
    mid INT
);

-- Insert sample data
INSERT INTO employee_manager (eid, name, mid) VALUES
(1, 'Amit', 4),
(2, 'Bhavana', 4),
(3, 'Chetan', 5),
(4, 'Farhan', NULL),
(5, 'Dinesh', NULL),
(6, 'Divya', 5);

SELECT T1.name AS e1, T1.mid
FROM employee_manager T1, employee_manager T2
WHERE T1.eid <> T2.eid
  AND T1.mid = T2.mid;
