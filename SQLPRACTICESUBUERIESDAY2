
CREATE TABLE Employees (
    emp_id INT,
    name VARCHAR(50),
    salary INT,
    department VARCHAR(50)
);

INSERT INTO Employees (emp_id, name, salary, department) VALUES
(1, 'Alice', 50000, 'HR'),
(2, 'Bob', 60000, 'IT'),
(3, 'Charlie', 70000, 'HR'),
(4, 'David', 55000, 'IT'),
(5, 'Eve', 65000, 'Sales'),
(6, 'Frank', 52000, 'Finance'),
(7, 'Grace', 48000, 'HR'),
(8, 'Hannah', 75000, 'IT'),
(9, 'Ian', 50000, 'Sales'),
(10, 'Jack', 58000, 'Finance');


SELECT NAME,salary
FROM Employees
WHERE SALARY>ANY(SELECT SALARY FROM EMPLOYEES WHERE department='HR');
SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY<ANY(SELECT SALARY FROM EMPLOYEES  WHERE DEPARTMENT='IT');

SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY=ANY(SELECT SALARY FROM EMPLOYEES  WHERE DEPARTMENT='Finance');


SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY>ANY(SELECT SALARY FROM EMPLOYEES  WHERE DEPARTMENT='SALES');

SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY<ANY(SELECT SALARY FROM EMPLOYEES  WHERE DEPARTMENT='FINANCE');

SELECT NAME,SALARY
FROM EMPLOYEES
WHERE DEPARTMENT='IT' AND SALARY >ANY(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='HR');
SELECT NAME,SALARY
FROM EMPLOYEES
WHERE DEPARTMENT ='SALES' AND SALARY <ANY(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='IT');

SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY >ANY(SELECT SALARY FROM EMPLOYEES);

SELECT NAME,salary
FROM EMPLOYEES
WHERE SALARY>ANY(SELECT SALARY FROM EMPLOYEES WHERE NAME='GRACE' );

SELECT NAME,SALARY 
FROM EMPLOYEES
WHERE SALARY >ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='HR');



SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY<ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='IT');


SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY=ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='SALES');



SELECT NAME,SALARY
FROM EMPLOYEES
WHERE SALARY>ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='FINANCE');


SELECT NAME, SALARY
FROM EMPLOYEES
WHERE SALARY< ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='SALES');



SELECT NAME, SALARY
FROM EMPLOYEES
WHERE SALARY!= ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='HR');

SELECT NAME,SALARY
FROM EMPLOYEES
WHERE DEPARTMENT='HR' AND SALARY> ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='SALES');


SELECT NAME,SALARY
FROM EMPLOYEES
WHERE DEPARTMENT='FINANCE' AND SALARY< ALL(SELECT SALARY FROM EMPLOYEES WHERE DEPARTMENT='IT');











